generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PromptLog {
  id             Int      @id @default(autoincrement())
  uniqueId       String   @unique
  initialPrompt  String
  enhancedPrompt String
  dateAndTime    DateTime
}
